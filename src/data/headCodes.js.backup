  return isScrolled.value ? 'w-8 h-8 text-blue-600' : 'w-10 h-10 text-white'
})

const textClass = computed(() => {
  return isScrolled.value ? 'text-xl text-gray-900' : 'text-2xl text-white'
})

const linkClass = computed(() => {
  return isScrolled.value 
    ? 'text-gray-700 hover:text-blue-600' 
    : 'text-white/90 hover:text-white'
})

const buttonClass = computed(() => {
  return isScrolled.value
    ? 'bg-blue-600 text-white hover:bg-blue-700'
    : 'bg-white/20 text-white hover:bg-white/30 backdrop-blur-sm border border-white/30'
})

const mobileMenuClass = computed(() => {
  return isScrolled.value
    ? 'bg-white/95 backdrop-blur-lg border-gray-200'
    : 'bg-white/10 backdrop-blur-lg border-white/20'
})

const toggleMobileMenu = () => {
  isMobileMenuOpen.value = !isMobileMenuOpen.value
}

const handleScroll = () => {
  scrollY.value = window.scrollY
}

onMounted(() => {
  window.addEventListener('scroll', handleScroll)
})

onUnmounted(() => {
  window.removeEventListener('scroll', handleScroll)
})
</script>`,

