const isMobileMenuOpen = ref(false)

const navLinks = [
  { name: 'Inicio' },
  { name: 'Servicios' },
  { name: 'Portfolio' },
  { name: 'Contacto' }
]

// Computed classes based on scroll position
const isScrolled = computed(() => scrollY.value > 50)

const headerClass = computed(() => {
  if (isScrolled.value) {
    return 'bg-white/95 backdrop-blur-lg shadow-xl border-b border-gray-200'
  }
  return 'bg-transparent'
})

const heightClass = computed(() => {
  return isScrolled.value ? 'h-14' : 'h-20'
})

